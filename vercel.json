{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@3.0.0"
    }
  },
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "dist/flynn_reservas/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/flynn_reservas/browser/$1" }
  ]
}
