{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  },
  "builds": [
    { "src": "api/**/*.ts", "use": "@vercel/node" },
    { "src": "dist/flynn_reservas/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/flynn_reservas/index.html" }
  ]
}

