{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/flynn_reservas",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@2.15.10",
      "nodeVersion": "18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/flynn_reservas/index.html" }
  ]
}
