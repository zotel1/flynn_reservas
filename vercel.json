{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/flynn-reservas",
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-node@20.0.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.ts" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
