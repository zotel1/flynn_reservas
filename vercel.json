{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/flynn_reservas/browser",
  "builds": [
    { "src": "api/**/*.{js,ts}", "use": "@vercel/node" },
    { "src": "dist/flynn_reservas/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/flynn_reservas/browser/index.html" }
  ]
}