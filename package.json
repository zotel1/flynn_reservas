{
  "name": "flynn-reservas",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "api": "vercel dev --yes",
    "dev": "concurrently \"ng serve\" \"npm run api\"",
    "generate:embeddings": "tsx scripts/generateEmbeddings.ts",
    "typecheck:api": "tsc -p api/tsconfig.json --noEmit",
    "test:api": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c jest.api.config.ts --passWithNoTests",
    "test:ui": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c jest.ui.config.ts --passWithNoTests",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@google/generative-ai": "^0.24.1",
    "@qdrant/js-client-rest": "^1.15.1",
    "@vercel/node": "^5.5.1",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "googleapis": "^165.0.0",
    "jsqr": "^1.4.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.7.0",
    "qdrant-client": "^0.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@types/estree": "^1.0.8",
    "@types/estree-jsx": "^1.0.5",
    "@types/express": "^5.0.5",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jasmine-core": "~5.9.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "jsdom": "^27.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  }
}
