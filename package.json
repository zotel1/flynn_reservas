{
  "name": "flynn-reservas",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ng serve",
    "api": "ts-node --project tsconfig.server.json server-local.ts",
    "dev": "concurrently \"npm run start\" \"npm run api\"",
    "generate:embeddings": "tsx scripts/generateEmbeddings.ts"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@google/generative-ai": "^0.24.1",
    "@qdrant/js-client-rest": "^1.15.1",
    "@vercel/node": "^5.5.1",
    "@xenova/transformers": "^2.17.2",
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2",
    "openai": "^6.7.0",
    "qdrant-client": "^0.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@types/express": "^5.0.5",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  }
}
